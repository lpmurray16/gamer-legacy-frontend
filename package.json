{
  "name": "gamer-legacy-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config-env": "node scripts/set-env.js",
    "start": "npm run config-env -- dev && ng serve",
    "build": "npm run config-env -- prod && ng build",
    "watch": "npm run config-env -- dev && ng build --watch --configuration development",
    "test": "npm run config-env -- dev && ng test",
    "local": "nvm use 22 && npm run config-env -- dev && ng serve"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "pocketbase": "^0.26.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "dotenv": "^17.2.3",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
